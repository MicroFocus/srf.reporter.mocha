{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "declaration": true,
        "noImplicitAny": false,
        "removeComments": true,
        "suppressExcessPropertyErrors": true,
        "typeRoots": [
            "node_modules/@types",
            "typings/custom"
        ]
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}
